
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model User{
  id  String @id @default(uuid())
  name String @unique
  email String @unique
  task Task[]

}

model Task{
  id String @id @default(uuid())
  created_at DateTime @default(now())
  title String
  body String
  isCompleted Boolean @default(false)
  user User @relation(fields: [userId], references: [id])
  userId String
}